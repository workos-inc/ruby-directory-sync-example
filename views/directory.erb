<% if @users.any? %>
  <h2>Users</h2>

  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user['username'] %>
      </li>
    <% end %>
  </ul>
<% end %>


<% if @groups.any? %>
  <h2>Groups</h2>

  <ul>
    <% @groups.each do |group| %>
      <li>
        <%= group['name'] %>
      </li>
    <% end %>
  </ul>
<% end %>
